{
  "$id": "https://exf.example.com/events/ExecutionReport.schema.json",
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "ExecutionReport",
  "type": "object",
  "required": ["eventId", "eventType", "version", "occurredAt", "orderId", "status"],
  "properties": {
    "eventId": { "type": "string" },
    "eventType": { "type": "string", "const": "ExecutionReport" },
    "version": { "type": "integer", "minimum": 1 },
    "occurredAt": { "type": "string", "format": "date-time" },
    "orderId": { "type": "string" },
    "status": { "type": "string", "enum": ["NEW", "PARTIALLY_FILLED", "FILLED", "CANCELLED", "REJECTED"] },
    "filledQty": { "type": "string" },
    "remainingQty": { "type": "string" },
    "lastFill": {
      "type": "object",
      "required": ["tradeId", "price", "quantity", "occurredAt"],
      "properties": {
        "tradeId": { "type": "string" },
        "price": { "type": "string" },
        "quantity": { "type": "string" },
        "occurredAt": { "type": "string", "format": "date-time" }
      },
      "additionalProperties": false
    },
    "reason": { "type": "string" }
  },
  "additionalProperties": false
}

